{
  "name": "almond",
  "packageManager": "yarn@3.0.2",
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "prettier:fix": "prettier './src/**/*.{tsx,ts}' --write",
    "eslint:fix": "eslint ./src --fix",
    "codeStyle:fix": "npm run prettier:fix && npm run eslint:fix",
    "husky": "pretty-quick --staged && yarn prettier && yarn tslint",
    "prettier": "prettier --write \"*/**/*.{js,ts,tsx,json}\"",
    "tslint": "tsc --noEmit && eslint \"*/**/*.{js,ts}\" --quiet",
    "test:unit": "cross-env NODE_ENV=testing jest -u --maxWorkers=4",
    "test:coverage": "yarn test:unit --coverage",
    "test:watch": "yarn test:unit --watch --verbose",
    "prepare": "husky install",
    "commit": "cz",
    "commit:signed": "cz -S",
    "clear-all": "rimraf ./.cache ./.next ./node_modules"
  },
  "dependencies": {
    "@emotion/cache": "^11.4.0",
    "@emotion/react": "^11.4.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.1",
    "@mui/material": "^5.0.1",
    "aos": "^2.3.4",
    "axios": "^0.21.4",
    "axios-cache-adapter": "^2.7.3",
    "card-validator": "^8.1.1",
    "dayjs": "^1.10.7",
    "formik": "^2.2.9",
    "jarallax": "^1.12.7",
    "js-cookie": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "next": "^11.1.2",
    "next-images": "^1.8.1",
    "next-offline": "^5.0.5",
    "next-pwa": "^5.3.1",
    "react": "^17.0.2",
    "react-countup": "^6.0.0",
    "react-dom": "^17.0.2",
    "react-ga": "^3.3.0",
    "react-images": "^1.1.7",
    "react-lazy-load-image-component": "^1.5.1",
    "react-redux": "^7.2.5",
    "react-slick": "^0.28.1",
    "react-spring": "^9.2.4",
    "react-syntax-highlighter": "^15.4.4",
    "react-typed": "^1.2.0",
    "react-visibility-sensor": "^5.1.1",
    "redux": "^4.1.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "slick-carousel": "^1.8.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-react-jsx": "^7.14.9",
    "@testing-library/dom": "^8.6.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/aos": "^3.0.4",
    "@types/js-cookie": "^2.2.7",
    "@types/jwt-decode": "^3.1.0",
    "@types/node": "^16.9.6",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-lazy-load-image-component": "^1.5.2",
    "@types/react-redux": "^7.1.18",
    "@types/redux-immutable-state-invariant": "^2.1.2",
    "@types/redux-mock-store": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^1.13.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^6.1.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.4.1",
    "prettier-eslint": "^13.0.0",
    "prettier-eslint-cli": "^5.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.3"
  }
}
